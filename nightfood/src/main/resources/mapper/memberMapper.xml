<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.nightfood.mapper.MemberMapper">
	<insert id="insetCafe">
		insert into cafeteria(idx,cafeterianm)
		values(CAFETERIA_IDX.nextval,#{cafeterianm})
	</insert>
 	<select id="selectCafe">
 		select *
 		from cafeteria
 	</select>
    
</mapper>


